{
  "extends": [
    "@electron-toolkit/tsconfig/tsconfig.web.json",
    "lynx-scanner/tsconfig.json"
  ],
  "include": [
    "lynx-scanner/src/logging.ts",
    "lynx-scanner/src/env.d.ts",
    "lynx-scanner/src/**/*",
    "lynx-scanner/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "lynx-scanner/*"
      ],
      "@api/*": ["lynx-scanner/src/store/gen/*"]
    }
  }
}
